      "a = date(2012, 1, 1)\n",
      "b = date(2012, 3, 10)\n",
      "\n",
      "for dt in rrule(DAILY, dtstart=a, until=b):\n",
      "\n",
      "        for town, kwarg in { \"Houston\": dict(date=dt,\n",
      "                                             lat='29.7632836', long='-95.3632715',\n",
